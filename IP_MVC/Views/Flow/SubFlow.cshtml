@model IEnumerable<BL.Domain.Flow>

@{
    ViewBag.Title = "SubFlows";
    Layout = "_Layout";
}

<div class="row row-cols-3">
    @foreach (var flow in Model)
    {
        <div class="col">
            <a href="">
                <div class="card">
                    <img src="https://via.placeholder.com/10x10" alt="Foto flow">
                    <div class="card-body">
                        <h5 class="card-title">@flow.Name</h5>
                        <p class="card-text">Description TODO</p>
                    </div>
                </div>
            </a>
        </div>
    }
</div>